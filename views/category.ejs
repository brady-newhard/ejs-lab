<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%- include('./partials/nav') %>
  <title><%= category %></title>
</head>
<body>
  <h1><%= category %></h1>
  <ul>
    <% menuItems.forEach(function(item) { %>
        <li>
            <b><%= item.name %></b>
            <br>$<%= item.price %>
            <br>Rating: <%= item.rating %>⭐️
            <br><%= item.details %>
        </li>
        <br>
        <% }) %>
</ul>
</body>
</html>
